{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"command": "haxe --run RunSetup",
			"label": "setup and tests",
			"problemMatcher": [
				"$haxe-absolute",
				"$haxe",
				"$haxe-error",
				"$haxe-trace"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
		},
	]
}